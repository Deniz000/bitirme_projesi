@NoArgsConstructor
@AllArgsConstructor
public class CreateExperienceRequest {
    private int candidateId;
    private int employerId;
    private int jobPositionId;
    private List<String> technologies;
    private LocalDate startDay;
    private LocalDate endDay;
}
